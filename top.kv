<RootScreen>:
	PrimeiroScreen:
	SegundoScreen:
	TerceiroScreen:
	
<PrimeiroScreen>:
	StackLayout:
		orientation: 'tb-lr'
		GridLayout:
			cols: 1
			size_hint: (1, .3)
			canvas.before:
				Rectangle:
					pos: self.pos
					size: self.size
					source: 'logo.png'
			
		GridLayout:
			cols: 3
			size_hint: (1, .4)
			padding: 20, 0
			spacing: 10, 0
			BubbleButton:
				text: "Criar\ntabela!"
				font_size: '18dp'
				bold: True
				on_release: root.manager.current = 'dois'
			
			BubbleButton:
				text: "Abrir\ntabela!"
				font_size: '18dp'
				bold: True
				on_press: root.abrir()
			BubbleButton:
				text: "Fotos das\nmáquinas!"
				font_size: '18dp'
				bold: True
				
		
